{
  "name": "@jcoreio/poll",
  "version": "0.0.0-development",
  "description": "handy promised-based polling API",
  "scripts": {
    "prepush": "npm test",
    "prepublishOnly": "echo This package is meant to be published by semantic-release from the dist build directory. && exit 1",
    "tc": "toolchain",
    "toolchain": "toolchain",
    "test": "toolchain test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "@jedwards1211/js-toolchain": {
    "mochaArgs": [
      "test/**.js",
      "--exit"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jcoreio/poll.git"
  },
  "keywords": [
    "poll",
    "retry"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcoreio/poll/issues"
  },
  "homepage": "https://github.com/jcoreio/poll#readme",
  "devDependencies": {
    "@jedwards1211/js-toolchain": "^2.0.7"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
